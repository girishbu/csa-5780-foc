#include <stdio.h>

int main() {
    int n, max, num;

    printf("Enter the number of integers: ");
    scanf("%d", &n);

    printf("Enter %d integers: ", n);
    scanf("%d", &max);

    for (int i = 1; i < n; i++) {
        scanf("%d", &num);
        if (num > max) {
            max = num;
        }
    }

    printf("The biggest number is %d.", max);

    return 0;
}

